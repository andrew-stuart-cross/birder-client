    "@types/node": "^18.11.8",
    "jasmine-core": "^4.5.0",
    "json-server": "^0.17.3",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "^5.0.4"
