{
  "name": "birder-client",
  "version": "0.0.0",
  "scripts": {
    "config": "ts-node -O '{\"module\": \"commonjs\"}' ./config.index.ts",
    "ng": "ng",
    "start": "ng serve",
    "api": "json-server src/api/db.json --port 3000 --routes src/api/routes.json --middlewares src/api/middleware.js",
    "build": "npm run config && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/cdk": "^18.0.1",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/google-maps": "^18.0.1",
    "@angular/localize": "^18.0.1",
    "@angular/material": "^18.0.1",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/router": "^18.0.1",
    "@auth0/angular-jwt": "^5.2.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "ng-mocks": "^14.12.2",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.2",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.1",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.12.13",
    "jasmine-core": "^5.1.2",
    "json-server": "^0.17.4",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "^5.4.5"
  }
}
